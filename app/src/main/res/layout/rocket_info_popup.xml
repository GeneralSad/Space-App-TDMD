<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        android:orientation="vertical">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/rocketPopupImage"
            />

        <TextView
            android:id="@+id/rocketPopupName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            android:textStyle="bold"
            />

        <TextView
            android:id="@+id/rocketPopupHeight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupDiameter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupMass"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupFS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupSS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupEngineType"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupEngineLoss"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupProp1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupProp2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupTWR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupType"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupActive"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupStages"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupBoosters"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupLaunchCost"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupSuccessRate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupCompany"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupWikipedia"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

        <TextView
            android:id="@+id/rocketPopupDescription"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name placeholder"
            android:textSize="20sp"
            />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>